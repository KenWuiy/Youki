<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="50dp"
    android:orientation="vertical"
    tools:context="mad.com.its02.fragment.ParkingFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="接口名称："
            />
        <TextView
            android:id="@+id/parking_textViewDescribe1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="接口URL："
            />
        <TextView
            android:id="@+id/parking_textViewDescribe2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="接口参数："
            />
        <TextView
            android:id="@+id/parking_textViewDescribe3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="接口描述："
            />
        <TextView
            android:id="@+id/parking_textViewDescribe4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            />

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="费率类型："
                 />
                <Spinner
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:id="@+id/parking_spinner"
                    android:background="@color/c1"
                    />

                <TextView
                    android:layout_width="86dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:text="单位计费额：" />

                <EditText
                    android:id="@+id/parking_money_edittext"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="0"
                     />
                <TextView
                    android:layout_width="15dp"
                    android:layout_height="wrap_content"
                    android:text="元" />

            </LinearLayout>
            <Button
                android:id="@+id/parking_setting_button"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="设置费率" />

            <TextView
                android:id="@+id/parking_setting_result"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:background="@color/c1"
                android:text="" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <Button
                android:id="@+id/parking_rate_query"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="查询费率" />

            <TextView
                android:id="@+id/parking_rate_result"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/c1"
                android:text="" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_weight="1"
            android:orientation="vertical">
            <Button
                android:id="@+id/parking_place_query"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="查询空闲车位" />

            <TextView
                android:id="@+id/parking_place_result"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/c1"
                android:text="" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
